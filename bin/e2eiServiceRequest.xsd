	<complexType name="customerType">
		<annotation>
			<documentation>customerType</documentation>
		</annotation>
			<sequence>
				<element name="alternateIdentifiers" minOccurs="0" maxOccurs="1">
					<annotation>
						<documentation>&lt;p style=&#34;margin-top: 0&#34;&gt;
  Used to capture alternate identifiers for the customer (CLE).
&lt;/p&gt;
&lt;p style=&#34;margin-top: 0&#34;&gt;

&lt;/p&gt;
&lt;p style=&#34;margin-top: 0&#34;&gt;
  Possibilities are NASPID, DUNS, GUDUNS, GCHID
&lt;/p&gt;</documentation>
						<appinfo>
							<label>alternateIdentifiers</label>
						</appinfo>
					</annotation>
					<complexType>
						<sequence>
							<element name="identifier" minOccurs="0" maxOccurs="unbounded" type="e2eiServiceRequest:specificationType"/>
						</sequence>
					</complexType>
				</element>
				<element name="billingAccounts" minOccurs="0" maxOccurs="1">
					<annotation>
						<documentation>&lt;p style=&#34;margin-top: 0&#34;&gt;
  A collection of billing accounts associated with this customer.
&lt;/p&gt;</documentation>
						<appinfo>
							<label>billingAccounts</label>
						</appinfo>
					</annotation>
					<complexType>
						<sequence>
							<element name="billingAccount" minOccurs="0" maxOccurs="unbounded" type="e2eiServiceRequest:billingAccount"/>
						</sequence>
					</complexType>
				</element>
				<element name="locations" minOccurs="0" maxOccurs="1">
					<annotation>
						<documentation>&lt;p style=&#34;margin-top: 0&#34;&gt;
  A collection of locations associated with this customer. These are
  typically service locations, not a billing address nor head office (GCH)
  address.
&lt;/p&gt;</documentation>
						<appinfo>
							<label>locations</label>
						</appinfo>
					</annotation>
					<complexType>
						<sequence>
							<element name="location" minOccurs="0" maxOccurs="unbounded" type="e2eiServiceRequest:locationType"/>
							<element name="fileAttachment" minOccurs="0" maxOccurs="unbounded" type="e2eiServiceRequest:attachmentType"/>
						</sequence>
					</complexType>
				</element>
				<element name="contacts" minOccurs="0" maxOccurs="1">
					<annotation>
						<documentation>&lt;p style=&#34;margin-top: 0&#34;&gt;
  A collection of contacts associated with this customer.
&lt;/p&gt;</documentation>
						<appinfo>
							<label>contacts</label>
						</appinfo>
					</annotation>
					<complexType>
						<sequence>
							<element name="contactRef" minOccurs="0" maxOccurs="unbounded" type="primitiveTypes:Int09"/>
						</sequence>
					</complexType>
				</element>
				<element name="revLoc" minOccurs="0" maxOccurs="1" type="primitiveTypes:String020"/>
				<element name="typeOfCustomer" minOccurs="0" maxOccurs="1" type="primitiveTypes:String032"/>
				<element name="addressRef" minOccurs="0" maxOccurs="1" type="primitiveTypes:Int09"/>
				<element name="marketSegment" minOccurs="0" maxOccurs="1" type="primitiveTypes:String032"/>
				<element name="solutions" minOccurs="0" maxOccurs="1">
					<annotation>
						<documentation>&lt;p style=&#34;margin-top: 0&#34;&gt;
  Used to capture solutions for the customer. By definition, solutions are
  not location specific.
&lt;/p&gt;</documentation>
						<appinfo>
							<label>solutions</label>
						</appinfo>
					</annotation>
					<complexType>
						<sequence>
							<element name="solution" minOccurs="0" maxOccurs="unbounded" type="e2eiServiceRequest:solutionType"/>
						</sequence>
					</complexType>
				</element>
				<element name="products" minOccurs="0" maxOccurs="1">
					<annotation>
						<documentation>&lt;p style=&#34;margin-top: 0&#34;&gt;
  Used to capture non location specific products for the customer. If the
  product is location specific, it will be captured under the pertainent
  location.
&lt;/p&gt;</documentation>
						<appinfo>
							<label>products</label>
						</appinfo>
					</annotation>
					<complexType>
						<sequence>
							<element name="product" minOccurs="0" maxOccurs="unbounded" type="e2eiServiceRequest:productType"/>
						</sequence>
					</complexType>
				</element>
			</sequence>
			<attribute name="id" type="primitiveTypes:Int09" use="required"/>
			<attribute name="customerName" type="primitiveTypes:String128"/>
			<attribute name="customerLegalName" type="primitiveTypes:String128"/>
		</complexType>

